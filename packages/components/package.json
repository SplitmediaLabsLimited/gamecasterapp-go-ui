  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/gamecasterapp/go-ui.git"
  },
